<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\Gloss_And_Bloom.ttf" />
    <None Remove="Fuente\Gloss_And_Bloom.ttf" />
    <None Remove="image0.png" />
    <None Remove="Imagenes\home.svg" />
    <None Remove="Imagenes\key-icon.png" />
    <None Remove="Imagenes\regalo.svg" />
    <None Remove="Sin título.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FontAwesome.Sharp" Version="6.3.0" />
    <PackageReference Include="MySql.Data" Version="8.0.33" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fuente\Gloss_And_Bloom.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Imagenes\image0.png" />
    <Resource Include="Imagenes\key-icon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Imagenes\Sin título.png" />
    <Resource Include="Imagenes\usuario.png" />
  </ItemGroup>

</Project>
